{
  "plugins": [
    "image-captions",
    "image-wrapper",
    "-sharing",
    "edit-link",
    "page-toc"
  ],
  "description": "Explains how to run Socialgorithm competitions or write games for it.",
  "author": "Socialgorithm",
  "language": "en",
  "pluginsConfig": {
    "edit-link": {
        "base": "https://github.com/socialgorithm/docs/edit/master/",
        "label": "Edit This Page"
    },
    "page-toc": {
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
      "position": "before-first",
      "showByDefault": false
    }
  }
}